{
  "name": "domain-osint-tool",
  "version": "1.0.0",
  "description": "Domain OSINT Scanner with MX records and technology stack analysis",
  "main": "app.py",
  "scripts": {
    "test": "pytest",
    "test:unit": "pytest tests/ -v",
    "test:coverage": "pytest --cov=core --cov=app --cov=utils --cov-report=term-missing",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:cypress:headed": "cypress run --headed",
    "test:all": "npm run test:unit && npm run test:cypress",
    "lint": "flake8 .",
    "format": "black .",
    "type-check": "mypy .",
    "quality": "npm run lint && npm run format && npm run type-check"
  },
  "keywords": [
    "osint",
    "domain",
    "mx-records",
    "technology-stack",
    "builtwith",
    "flask",
    "python"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.6.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}